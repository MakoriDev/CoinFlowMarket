  setInterval(function() {
    $.ajax({
      url: "../ajax/function",
      type: "POST",
      data: {
        action: "REQUEST_TOKEN"
      },
      success: function(response) {
        if(response == "timeout") {
          setTimeout(function() {
            location.replace("../logout");
          }, 200);
        }
      }
    })
  }, 5000);
</script>
