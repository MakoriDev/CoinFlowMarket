  setTimeout(function() {
    $(".noti-" + status).addClass("notification-active");
  }, 50);
  

  setTimeout(function() {
    $(".noti-" + status).removeClass("notification-active");
    setTimeout(function() {
        $(".noti-" + status).remove();
    }, 250);
    
  }, 3500);
}
