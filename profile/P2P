<script type="text/javascript">
    $("#close_modal_btn").on("click", function() {
        $("#p2p_error_modal").removeClass("withdrawal-successful-active");
    });

    $("#close_modal").on("click", function() {
        $("#p2p_error_modal").removeClass("withdrawal-successful-active");
    });

    function openP2P() {
        $("#p2p_error_modal").addClass("withdrawal-successful-active");
    }
</script>